<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.flyread.com.cn/columnComboBox"
           xmlns="http://www.flyread.com.cn/columnComboBox"
           xmlns:l="http://www.flyread.com.cn/comboBox"
           elementFormDefault="qualified">

    <xs:import namespace="http://www.flyread.com.cn/comboBox"/>

    <xs:element name="args" substitutionGroup="l:args" type="columnComboBoxArgs" />
    <xs:complexType name="columnComboBoxArgs" >
        <xs:complexContent>
            <xs:extension base="l:comboBoxArgs">
                <xs:attribute name="tableField" type="xs:string"  />
                <xs:attribute name="columnNum" type="xs:string" />
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
</xs:schema>


